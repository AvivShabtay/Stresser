{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "autoprefixer": "^9.7.6",
    "axios": "^0.19.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.4.0"
  },
  "scripts": {
    "build:tailwind": "postcss src/App.css -o src/tailwind.generated.css",
    "watch:tailwind": "postcss -w src/App.css -o src/tailwind.generated.css",
    "start": "run-p watch:tailwind start:react",
    "start:react": "set PORT=8000 && set REACT_APP_SERVER_URL=http://localhost:3000 && react-scripts start",
    "prebuild": "npm run build:tailwind",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier -l \"**/*.{js,jsx}\"",
    "prettier:write": "prettier --write \"**/*.{js,jsx}\"",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^4.7.0"
  },
  "proxy": "http://localhost:8000"
}
